<Window x:Class="RegistroCompleto2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RegistroCompleto2"
        mc:Ignorable="d"
        Title="Menu Principal" Height="400" Width="600">
    <Grid>
        <StackPanel Orientation="Vertical">
            <Menu Height="18">
                <MenuItem Header="Registros">
                    <MenuItem Header="Usuarios" x:Name="registroUsuario" Click="registroUsuario_Click"></MenuItem>
                </MenuItem>
            </Menu>
            
        </StackPanel>

    </Grid>
</Window>
